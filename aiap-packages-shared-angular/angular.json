{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client-shared-carbon": {
      "projectType": "library",
      "root": "projects/client-shared-carbon",
      "sourceRoot": "projects/client-shared-carbon/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/client-shared-carbon/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/client-shared-carbon/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/client-shared-carbon/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/client-shared-carbon/src/test.ts",
            "tsConfig": "projects/client-shared-carbon/tsconfig.spec.json",
            "karmaConfig": "projects/client-shared-carbon/karma.conf.js"
          }
        }
      }
    },
    "client-shared-utils": {
